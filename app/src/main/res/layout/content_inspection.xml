<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:layout_marginTop="57dp"
    tools:context="com.esdrasmorais.inspetoronline.ui.inspection.InspectionReportActivity">

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_line"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_line">

        <AutoCompleteTextView
            android:id="@+id/line_dropdown"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_prefix"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_prefix">

        <AutoCompleteTextView
            android:id="@+id/prefix_dropdown"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:numeric="integer"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_location"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_location">

        <AutoCompleteTextView
            android:id="@+id/location_dropdown"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_vehicle_state"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_vehicle_state">

        <AutoCompleteTextView
            android:id="@+id/vehicle_state_dropdown"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_presentation_employees"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_presentation_employees">

        <AutoCompleteTextView
            android:id="@+id/presentation_employees"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_vehicle_identification"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_vehicle_identification">

        <AutoCompleteTextView
            android:id="@+id/vehicle_identification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_personal_objects_cleaning"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_personal_objects">

        <AutoCompleteTextView
            android:id="@+id/personal_objects_cleaning"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_vehicle_objects_conservation"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_vehicle_objects_conservation">

        <AutoCompleteTextView
            android:id="@+id/vehicle_objects_conservation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_employee_identification"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_employee_identification">

        <AutoCompleteTextView
            android:id="@+id/employee_identification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_wheelchair_seat_belt"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_wheelchair_seat_belt">

        <AutoCompleteTextView
            android:id="@+id/wheelchair_seat_belt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_objects_forbiden_to_role"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_objects_forbiden_to_role">

        <AutoCompleteTextView
            android:id="@+id/objects_forbiden_to_role"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_vehicle_security_accessories"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_vehicle_security_accessories">

        <AutoCompleteTextView
            android:id="@+id/vehicle_security_accessories"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_impediment_to_inspection"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_impediment_to_inspection">

        <AutoCompleteTextView
            android:id="@+id/impediment_to_inspection"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.ExposedDropdownMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/hint_inspection_rate"
        android:layout_marginBottom="17dp"
        android:id="@+id/input_layout_inspection_rate">

        <AutoCompleteTextView
            android:id="@+id/inspection_rate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/input_layout_comments"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="17dp">

        <EditText
            android:id="@+id/edit_text_comments"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/hint_comments"
            android:inputType="textMultiLine" /><!--android:maxLines="10"
            android:minLines="5"-->

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_save"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/text_save"
        android:layout_marginBottom="77px"
        android:textAlignment="center"/>

<!--</android.support.constraint.ConstraintLayout>-->
</LinearLayout>